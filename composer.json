{
    "name": "alt-three/login",
    "description": "An OAuth 2 Provider For Laravel 5",
    "keywords": ["Login", "GitHub", "GitLab", "Bitbucket", "OAuth2", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*",
        "illuminate/http": "5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "graham-campbell/guzzle-factory": "^3.0",
        "graham-campbell/manager": "^4.1"
    },
    "require-dev": {
        "alt-three/uuid": "^1.0",
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.1",
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "suggest": {
        "alt-three/uuid": "Required to use the bitbucket provider (^1.0)."
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Login\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Login\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        },
        "laravel": {
            "providers": [
                "AltThree\\Login\\LoginServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
