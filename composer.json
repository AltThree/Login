{
    "name": "alt-three/login",
    "description": "A GitHub Login Provider For Laravel 5",
    "keywords": ["Login", "GitHub", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/http": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "guzzlehttp/guzzle": "^5.3.1|^6.2.1"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^1.1",
        "graham-campbell/testbench": "^4.0",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Login\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Login\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "AltThree\\Login\\LoginServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
